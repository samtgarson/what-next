{
  "name": "what-next",
  "version": "0.0.1",
  "description": "Find what to do next with the power of emojis",
  "author": "Sam Garson <samtgarson@gmail.com>",
  "license": "MIT",
  "nativescript": {
    "id": "com.samgarson.whatnext",
    "tns-android": {
      "version": "5.1.0"
    },
    "tns-ios": {
      "version": "5.1.1"
    }
  },
  "scripts": {
    "lint": "eslint ./app",
    "build:ios": "tns build ios --bundle --release --forDevice --teamId SZR99J9ZFT --env.production --env.uglify",
    "start:ios": "tns run ios --hmr --bundle",
    "clean": "rm -rf ./platforms"
  },
  "dependencies": {
    "@tweenjs/tween.js": "^17.2.0",
    "nativescript-geolocation": "^4.3.1",
    "nativescript-socket.io": "^0.9.0",
    "nativescript-vue": "^2.0.0",
    "tns-core-modules": "^5.1.2",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@samtgarson/eslint-config": "^1.0.3",
    "@vue/devtools": "5.0.0-beta.3",
    "babel-loader": "^8.0.2",
    "babel-traverse": "6.26.0",
    "babel-types": "6.26.0",
    "babylon": "6.18.0",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "dotenv": "^6.2.0",
    "eslint": "^5.0.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-import-resolver-jest": "^2.1.1",
    "eslint-plugin-html": "^5.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.1.3",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-vue": "^5.0.0",
    "lazy": "1.0.11",
    "nativescript-dev-webpack": "^0.19.1-next-2019-01-14-120258-01",
    "nativescript-toast": "^1.4.6",
    "nativescript-vue-devtools": "^1.0.0",
    "nativescript-vue-template-compiler": "^2.0.0",
    "nativescript-worker-loader": "~0.9.0",
    "node-sass": "^4.9.2",
    "prettier": "prettier/prettier#master",
    "sass-loader": "^7.1.0",
    "terser-webpack-plugin": "^1.1.0",
    "vue-loader": "^15.2.6",
    "webpack": "^4.16.4",
    "webpack-bundle-analyzer": "~2.13.1",
    "webpack-cli": "^3.1.0"
  },
  "eslintConfig": {
    "extends": [
      "@samtgarson",
      "plugin:vue/strongly-recommended"
    ],
    "rules": {
      "vue/html-indent": [
        "error",
        2
      ],
      "promise/prefer-await-to-callbacks": 0,
      "promise/prefer-await-to-then": 0,
      "vue/max-attributes-per-line": [
        "error",
        {
          "singleline": 3,
          "multiline": 3
        }
      ],
      "vue/component-name-in-template-casing": 0,
      "vue/attribute-hyphenation": 0,
      "vue/singleline-html-element-content-newline": 0
    },
    "globals": {
      "TNS_ENV": false
    }
  }
}
